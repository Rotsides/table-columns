{
  "name": "@rotsides/table-columns",
  "version": "0.1.4",
  "type": "module",
  "files": ["dist", "index.d.ts"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "": ["index.d.ts"],
      "*": ["index.d.ts"]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown && cp dist/index.d.mts dist/index.d.ts",
    "prepack": "pnpm build",
    "dev": "tsdown --watch",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.0.0"
  }
}
